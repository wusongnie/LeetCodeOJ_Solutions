class Solution {
    func twoSum(_ nums: [Int], _ target: Int) -> [Int] {
        var array=[0,0]
        for i in 0..<nums.count {
            
            for j in i+1..<nums.count{
              if(nums[i]+nums[j]==target){
                array[0]=i
                array[1]=j
                return array
                  
              }
            }
        }
        return array
    }
}
